tagVersion=$($binDir/findOutDeployTag $binDir/../README.org)
#echo $tagVersion
$(dirname $0)/installationTest || exit 1000
echo Check the status
git commit  --dry-run -a -m "Riepilogo" || echo ""
read -e  -p 'Proceed (y/n)?' -i y reply
test "$reply" != "y"  && exit
echo Commit...
git status

